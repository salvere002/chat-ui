    "@types/node": "^22.14.1",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^8.2.2",
    "typescript": "^5.8.3",
    "vite": "^6.2.6"
  },
  "dependencies": {
    "@types/axios": "^0.14.4",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.2",
    "@types/react-syntax-highlighter": "^15.5.13",
    "axios": "^1.8.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "remark-gfm": "^4.0.1",
    "zustand": "^5.0.3"
  }
